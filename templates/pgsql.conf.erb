<% if vsname %>
vsname = <%= vsname %>
<% end %>
<% if backupdir %>
backupdir = <%= backupdir %>
<% end %>
<% if databases.is_a? String -%>
<%= 'databases = ' + databases %>
<% elsif databases.is_a? Array -%>
<%= "databases = " + databases.map { |i| "#{i}" }.join(" ") %>
<% end -%>
compress = <%= compress ? 'yes' : 'no' %>

